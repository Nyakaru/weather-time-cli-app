{
  "name": "weather-time-cli-app",
  "version": "1.0.0",
  "description": "Given an array of inputs (location name, postal code), log the current time and weather for those locations",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "tsc --outDir dist ./index.ts && node ./dist/index.js ",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nyakaru/weather-time-cli-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nyakaru/weather-time-cli-app/issues"
  },
  "homepage": "https://github.com/Nyakaru/weather-time-cli-app#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "luxon": "^1.21.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.0.23",
    "@types/luxon": "^1.21.0",
    "@types/node": "^12.12.11",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).(ts|js)",
      "<rootDir>/**/?(*.)(spec|test).(ts|js)"
    ]
    
  }
}
